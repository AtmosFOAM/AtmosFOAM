// dummy file to placate paraView
FoamFile {
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}

offCentre 0.5;
SIgravityWaves false;

interpolationSchemes {
    default         none;
    interpolate(theta) APVM Uf; //midPoint;
    interpolate(volIntegrate((snGrad(theta)*magSf))) midPoint;
    interpolate(rho) midPoint;
    interpolate(convection(U,u)) midPoint;
    H midPoint;
    interpolate(u) midPoint;
    interpolate(grad(thetaf)) midPoint;
    thetaFromb midPoint;
}

divSchemes {
    default         none;
//    div(U,u)        Gauss cubicUpwindCPCFit false;
//    div(U,theta)    Gauss cubicUpwindCPCFit false;
    div(U,u)        Gauss linearUpwind grad(u);
    div(U,theta)    Gauss APVM Uf; //linearUpwind grad(theta);
}

gradSchemes {
    default         none;
    grad(theta)     Gauss midPoint;
    grad(Exner)     Gauss midPoint;
    grad(u)         Gauss midPoint;
    grad(theta)     Gauss midPoint;
}

laplacianSchemes {
    default         Gauss linear uncorrected;
}

ddtSchemes {
    default         none;
    ddt(rho)        Euler;
    ddt(rho,theta)  Euler;
    ddt(Psi,Exner)  Euler;
}

snGradSchemes
{
    default         none;
    snGrad(Exner)   uncorrected;
    snGrad(theta)   uncorrected;
}

fluxRequired
{
    default         no;
    Exner;
}
