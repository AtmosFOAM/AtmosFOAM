FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler;
}
offCentre 0.5;
SIgravityWaves false;

gradSchemes
{
    default         none;
    linearUpwind    Gauss linear;
    grad(thetaRho)  Gauss linear;
}

divSchemes
{
    div(un,airVapourRho)   Gauss linearUpwind linearUpwind;
    div(un,airLiquidFrac)   Gauss linearUpwind linearUpwind;
    div(un,waterVapourRho)   Gauss linearUpwind linearUpwind;
    div(un,waterLiquidFrac)   Gauss linearUpwind linearUpwind;
}

laplacianSchemes
{
    default         Gauss linear uncorrected;
}

interpolationSchemes
{
    default        midPoint;
}

snGradSchemes
{
    default         none;
    snGrad(Exner)   uncorrected;
    snGrad(kappam)   uncorrected;
}

fluxRequired
{
    default         no;
    Exner;
}


