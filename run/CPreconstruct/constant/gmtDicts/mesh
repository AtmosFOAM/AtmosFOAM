FoamFile {
        version         1.4;
        format          ascii;

        root            "";
        case            "";
        instance        "constant";
        local           "";

        class           dictionary;
        object          gmtDict;
}

projection "X5i/5i";
projection2D XZ;
plotBoundaryDots false;
plotZeroValuePolys false;

FieldsToPlot (
      [none mesh "gray"]
      [Tf rawValues 0 10 1 "$GMTU/colours/white_red.cpt"]
      [Tf rawBoundaryValues 0 10 1 "$GMTU/colours/white_red.cpt"]
      [none numbered 0]
);

region "-1/4/-1/4";
