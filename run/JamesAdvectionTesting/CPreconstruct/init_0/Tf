FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "0";
    object      Tf;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   nonuniform 
(
0
1
0
5
3
0
0
);

boundaryField
{
    left
    {
        type            cyclic;
        value           uniform 0;
    }
    right
    {
        type            cyclic;
        value           uniform 0;
    }
    ground
    {
        type            calculated;
        value           nonuniform (4 1 2);
    }
    top
    {
        type            calculated;
        value           nonuniform (4 3 7);
    }
    frontAndBack
    {
        type            empty;
        value           nonuniform 0();
    }
}


