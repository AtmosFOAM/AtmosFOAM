FoamFile
{
    version         2.3;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XZ;

FieldsToPlot
(
    [p rawValues 0 0 0 "$GMTU/colours/red_white_blue.cpt"]
    [Uf vectors 50 3 "black"]
);

separateLegend true;
region "-10e3/10e3/0/25e3";
annotate "minMax -1.2e3 -400";
boundaryMarks "2500/2500";

